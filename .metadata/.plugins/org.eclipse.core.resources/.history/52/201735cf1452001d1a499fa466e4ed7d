		int limit = scanner.nextInt();
		int array[] = new int[(limit-1)/2];
		int sum = 0;
		int start = 1;
		for(int j=0; j <= (limit-1)/2; j++)
		{
			array[j] = start;
			start = start + 2;
		}
		
		for(int i=0; i<6; i++)
		{
			System.out.println(array[i]);
		}
		
		
